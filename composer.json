{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-cors": "^0.11.2",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "barryvdh/laravel-translation-manager": "^0.5.0",
        "caouecs/laravel-lang": "~3.0",
        "doctrine/dbal": "^2.5",
        "eluceo/ical": "^0.13.0",
        "froiden/envato": "^1.0",
        "froiden/laravel-installer": "^1.6",
        "froiden/laravel-rest-api": "^6.0",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "laravel-notification-channels/onesignal": "^2.0.0",
        "laravel/cashier": "^10.5",
        "laravel/framework": "6.20.14",
        "laravel/helpers": "^1.1",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "~3.1.0",
        "nwidart/laravel-modules": "^6.0",
        "paragonie/random_compat": "~1.4",
        "paypal/rest-api-sdk-php": "*",
        "pcinaglia/laraupdater": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "razorpay/razorpay": "^2.5",
        "spatie/laravel-cookie-consent": "^2.11",
        "stichoza/google-translate-php": "^4.0",
        "trebol/entrust": "0.3",
        "tymon/jwt-auth": "^1.0.0",
        "yajra/laravel-datatables-buttons": "~4.6",
        "yajra/laravel-datatables-html": "~4.4",
        "yajra/laravel-datatables-oracle": "^9.0",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": ["app/Helper/start.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
